{
  "name": "uno-demo",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=18",
    "pnpm": ">=6"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "check:type": "tsc --project tsconfig.dev.json --noEmit",
    "lint:main": "eslint '**/*.{js,ts,tsx}'",
    "dev-js": "NODE_ENV=development node -r esbuild-register esbuild.config.ts",
    "dev-css": "unocss \"src/**/*.{html,tsx}\" -c unocss.config.ts -o src/uno-gen.css -w",
    "dev": "concurrently --raw --group --kill-others-on-fail 'pnpm:dev-*'",
    "build-js": "NODE_ENV=production node -r esbuild-register esbuild.config.ts",
    "build-css": "unocss \"src/**/*.{html,tsx}\" -c unocss.config.ts -o src/uno-gen.css",
    "build": "pnpm build-css && pnpm build-js"
  },
  "browser": {
    "path": "path-browserify"
  },
  "devDependencies": {
    "@iconify-json/twemoji": "^1.1.7",
    "@iconify/utils": "^2.0.6",
    "@svgr/core": "^6.5.1",
    "@svgr/plugin-svgo": "^6.5.1",
    "@types/d3-format": "^3.0.1",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "@unocss/cli": "~0.46.5",
    "@unocss/preset-icons": "~0.46.5",
    "@unocss/transformer-compile-class": "~0.46.5",
    "chalk": "^5.2.0",
    "chokidar": "^3.5.3",
    "concurrently": "^7.6.0",
    "dotenv": "^16.0.3",
    "esbuild": "^0.16.12",
    "esbuild-register": "^3.4.2",
    "escape-string-regexp": "^5.0.0",
    "eslint": "^8.30.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fs-extra": "^11.1.0",
    "prettier": "^2.8.1",
    "typescript": "^4.9.4",
    "unocss": "~0.46.5",
    "vite": "3",
    "workbox-build": "^6.5.4",
    "yalive-server": "^0.2.5"
  },
  "dependencies": {
    "@ant-design/icons": "^4.8.0",
    "@radix-ui/colors": "^0.1.8",
    "@reduxjs/toolkit": "^1.9.1",
    "@stitches/react": "^1.2.8",
    "ahooks": "^3.7.4",
    "antd": "^5.1.1",
    "d3-format": "^3.1.0",
    "dayjs": "^1.11.7",
    "history": "^5.3.0",
    "ofetch": "^1.0.0",
    "path-browserify": "^1.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.6.1",
    "uuid": "^9.0.0"
  }
}
